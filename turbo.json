{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".svelte-kit/**", "build/**", "dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "check": {
      "dependsOn": ["^build"]
    },
    "lint": {
      "dependsOn": ["^build"]
    },
    "format": {},
    "preview": {
      "dependsOn": ["build"]
    }
    ,
    "storybook": {
      "cache": false,
      "persistent": true
    },
    "build:storybook": {
      "dependsOn": [],
      "outputs": ["storybook-static/**"]
    }
  }
}
