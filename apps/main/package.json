{
  "name": "main",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "pnpm -w eslint apps/main/src --ext .js,.ts,.svelte",
    "format": "pnpm -w prettier --write apps/main",
    "gen:validate": "pnpm run format && pnpm run lint && pnpm run check",
    "test": "vitest run",
    "test:ui": "vitest",
  "storybook": "storybook dev -p 6006 --no-open",
    "build:storybook": "storybook build"
  },
  "devDependencies": {
    "@sveltejs/adapter-vercel": "^5.10.2",
    "@sveltejs/kit": "^2.5.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "svelte-check": "^4.0.0",
    "svelte-preprocess": "^5.1.4",
    "typescript": "^5.0.0",
    "vite": "^5.4.0",
    "@storybook/sveltekit": "^8.2.0",
    "@storybook/addon-essentials": "^8.2.0",
    "@storybook/addon-a11y": "^8.2.0",
    "drizzle-kit": "^0.26.0",
    "@types/node": "^20.11.30",
    "@types/pg": "^8.11.6"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.0.1",
    "@supabase/supabase-js": "^2.57.4",
    "jszip": "^3.10.1",
    "monaco-editor": "^0.52.0",
    "openai": "^4.67.0",
    "socket.io-client": "^4.8.0",
    "pg": "^8.13.1",
    "drizzle-orm": "^0.33.0",
    "svelte": "^5.0.0",
    "@vercel/blob": "^0.23.0"
  },
  "type": "module"
}